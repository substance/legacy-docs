    "4541db10ccc40347d88174919a0d6135": {
      "type": "text",
      "id": "4541db10ccc40347d88174919a0d6135",
      "source_id": "",
      "content": "You can easily embed videos in your publication. Substance supports modern HTML5 video formats."
    },
    "heading_954ae3118fe6be7d105cf4bea100659b": {
      "type": "heading",
      "id": "heading_954ae3118fe6be7d105cf4bea100659b",
      "source_id": "",
      "content": "Audio",
      "level": 2
    },
    "b34d95f44ac759789ecf0f3142228a5e": {
      "type": "text",
      "id": "b34d95f44ac759789ecf0f3142228a5e",
      "source_id": "",
      "content": "Similar to video you can also include sound files in your documents."
    },
    "c1869aa50390818841d4603628e6070d": {
      "type": "text",
      "id": "c1869aa50390818841d4603628e6070d",
      "source_id": "",
      "content": "Please note that due to licensing issues MP3 files can't be played within the Composer as well some browsers. The recommended way is to add two versions of the same audio: MP3 and OGG."
    },
    "figure_c19ade1df7331af16d8651ca029f10f0.gif": {
      "type": "file",
      "id": "figure_c19ade1df7331af16d8651ca029f10f0.gif",
      "version": 3,
      "content_type": "image/gif"
    },
    "text_3d1565553b3ba3d1629443d99206c569": {
      "type": "text",
      "id": "text_3d1565553b3ba3d1629443d99206c569",
      "source_id": "",
      "content": "A video node filled with a MP4 and a WebM version of a video"
    },
    "figure_c19ade1df7331af16d8651ca029f10f0": {
      "type": "figure",
      "id": "figure_c19ade1df7331af16d8651ca029f10f0",
      "image": "figure_c19ade1df7331af16d8651ca029f10f0.gif",
      "caption": "text_3d1565553b3ba3d1629443d99206c569"
    },
    "41784737155673383c31fce5371a9bbe": {
      "type": "text",
      "id": "41784737155673383c31fce5371a9bbe",
      "source_id": "",
      "content": "Unfortunately, browsers support different encodings for video playback. We recommend to use a combination of MP4 (h264) and WebM for maximum browser compatibility."
    },
    "figure_d022d3309dc8b774d5b8b33c5f1b141c.gif": {
      "type": "file",
      "id": "figure_d022d3309dc8b774d5b8b33c5f1b141c.gif",
